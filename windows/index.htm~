var pic = new Image();

var f = 1;
function timeShow()
 {
	f++;
	if(f == 19){f=1};	
	var picSrc = 'i/pic'+ f + '.jpg';

	pic.src = picSrc;
	
	document.place.src = pic.src;
;
	setTimeout("timeShow()", 1 * 1000);
	

}

	

